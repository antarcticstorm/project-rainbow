{"ast":null,"code":"import promoData from '@/json/promos.json';\nimport PopupModal from '@/components/PopupModal.vue';\nexport default {\n  name: 'PromosView',\n  props: {\n    promoType: String\n  },\n  data() {\n    return {\n      promos: promoData,\n      isOpen: false,\n      popupType: \"promo\",\n      popupData: []\n    };\n  },\n  components: {\n    PopupModal\n  },\n  methods: {\n    modalId(x) {\n      return x.slug + \"-\" + x.id;\n    },\n    showModal(json) {\n      this.isOpen = true, this.popupType = \"promo\", this.popupData = json;\n    }\n  }\n};","map":{"version":3,"names":["promoData","PopupModal","name","props","promoType","String","data","promos","isOpen","popupType","popupData","components","methods","modalId","x","slug","id","showModal","json"],"sources":["/Users/karahalbert/Sites/rainbow/src/components/PromosView.vue"],"sourcesContent":["<template>\n    <p>{{promoType}}</p>\n    <table id=\"search-table\">\n        <thead>\n            <tr>\n                <th>\n                    <span class=\"flex items-center\">\n                        Name\n                    </span>\n                </th>\n                <th>\n                    <span class=\"flex items-center\">\n                        Ticker\n                    </span>\n                </th>\n                <th>\n                    <span class=\"flex items-center\">\n                        Stock Price\n                    </span>\n                </th>\n                <th>\n                    <span class=\"flex items-center\">\n                        Market Capitalization\n                    </span>\n                </th>\n            </tr>\n        </thead>\n        <tbody>\n            <template v-for=\"promo in promos\" :key=\"promo.id\">\n                <tr v-if=\"promo.approved == 'Yes' && promo.promo_type == promoType\">\n                    <td class=\"font-medium text-gray-900 whitespace-nowrap dark:text-white\" @click=\"showModal(promo)\">{{promo.name}}</td>\n                    <PopupModal ref=\"modal\" :key=\"promo.id\" :isOpen=\"false\" popupType=\"promo\" :popupData=\"promo\"></PopupModal>\n                </tr>\n            </template>\n        </tbody>\n    </table>\n\n</template>\n\n<script>\nimport promoData from '@/json/promos.json'\nimport PopupModal from '@/components/PopupModal.vue'\nexport default {\n    name: 'PromosView',\n    props: {\n        promoType: String\n    },\n    data() {\n        return { \n            promos: promoData,\n            isOpen: false,\n            popupType: \"promo\",\n            popupData: []\n        }\n    },\n    components: {\n        PopupModal,\n    },\n    methods: {\n        modalId(x) {\n            return x.slug + \"-\" + x.id;\n        },\n        showModal(json) {\n            this.isOpen = true,\n            this.popupType = \"promo\",\n            this.popupData = json\n        }\n    }\n}\n</script>"],"mappings":"AAwCA,OAAOA,SAAQ,MAAO,oBAAmB;AACzC,OAAOC,UAAS,MAAO,6BAA4B;AACnD,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACHC,SAAS,EAAEC;EACf,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAEP,SAAS;MACjBQ,MAAM,EAAE,KAAK;MACbC,SAAS,EAAE,OAAO;MAClBC,SAAS,EAAE;IACf;EACJ,CAAC;EACDC,UAAU,EAAE;IACRV;EACJ,CAAC;EACDW,OAAO,EAAE;IACLC,OAAOA,CAACC,CAAC,EAAE;MACP,OAAOA,CAAC,CAACC,IAAG,GAAI,GAAE,GAAID,CAAC,CAACE,EAAE;IAC9B,CAAC;IACDC,SAASA,CAACC,IAAI,EAAE;MACZ,IAAI,CAACV,MAAK,GAAI,IAAI,EAClB,IAAI,CAACC,SAAQ,GAAI,OAAO,EACxB,IAAI,CAACC,SAAQ,GAAIQ,IAAG;IACxB;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}